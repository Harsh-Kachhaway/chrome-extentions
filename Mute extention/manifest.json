{
  "manifest_version": 3,
  "name": "Tab Mute Manager ",
  "version": "1.1.1",
  "description": "mutes unwanted website ",
  "permissions": ["storage", "activeTab", "scripting","tabs"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "newbackground.js"
  },
  "icons": {
"16": "icons/icon16.png",
"48": "icons/icon48.png",
"128": "icons/icon128.png"
}
}
